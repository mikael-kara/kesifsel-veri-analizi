    <kütüphane adı="numpy" />
    <kütüphane adı="seaborn" />
    <kütüphane adı="matplotlib" />
  </kütüphaneler>
  <veri_olusumu>
    <veri_seti adı="df1" satır_sayısı="100" sütun_sayısı="3">
