    <temel_istatistik veri_seti="df1" />
    <temel_istatistik veri_seti="df2" />
    <eksik_veri veri_seti="df1" />
    <eksik_veri veri_seti="df2" />
    <kategorik_degisken veri_seti="df1" sütun_adı="C" />
  </veri_inceleme>
  <veri_görselleştirme>
    <histogram veri_seti="df1" sütunlar="tüm sayısal sütunlar" />
    <pairplot veri_seti="df2" değişkenler="tüm sayısal değişkenler" renk="tür" />
    <korelasyon_matrisi veri_seti="df2" />
  </veri_görselleştirme>
